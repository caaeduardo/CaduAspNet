<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Cadu Enterprise Inc.</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PrimeiroProjeto.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/Lux_bootstrap.min.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-light bg-black border-bottom box-shadow mb-3">
            <div class="container-fluid p-0">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">PrimeiroProjeto</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon bg-white"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-around">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Cadastro">Register</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-white" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="true">Other Options</a>
                            <div class="dropdown-menu text-green">
                                <a class="dropdown-item" href="#">New Register</a>
                                <a class="dropdown-item" href="#">Cadu for Cadu</a>
                                <a class="dropdown-item" href="#">Dashboard</a>
                                <a class="dropdown-item" href="#">Service Order</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" href="#">Settings</a>
                                <a class="dropdown-item" href="https://www.google.com/search?gs_ssp=eJzj4tLP1Tcwi680rDAwYPTiLkgszcksLklUSDQBAF5-B5U&q=paulista+a4&rlz=1C1GCEU_pt-BRBR1156BR1156&oq=psulidta+&gs_lcrp=EgZjaHJvbWUqCQgDEC4YDRiABDIGCAAQRRg5MgwIARAuGA0Y1AIYgAQyCQgCEC4YDRiABDIJCAMQLhgNGIAEMgkIBBAuGA0YgAQyDwgFEC4YDRivARjHARiABDIJCAYQLhgNGIAEMgkIBxAAGA0YgAQyCQgIEAAYDRiABNIBCTcxNzBqMGoxNagCCLACAfEF17nQwYGJnX0&sourceid=chrome&ie=UTF-8#sie=m">Exit</a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
         </nav>
    </header>
    <div class="container bg-success-subtle">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Remove the container if you want to extend the Footer to full width. -->
    <div class="container-fluid p-0 ">
        <!-- Footer -->
        <footer class="text-center text-white" style="background-color: #DBF2E3">
            <!-- Grid container -->
            <div class="container">
                <!-- Section: Links -->
                <section class="mt-5">
                    <!-- Grid row-->
                    <div class="row text-center d-flex justify-content-center pt-5">
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a href="#!" class="text-black">About us</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a href="#!" class="text-black">Products</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a href="#!" class="text-black">Awards</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a href="#!" class="text-black">Help</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-2">
                            <h6 class="text-uppercase font-weight-bold">
                                <a href="#!" class="text-black">Contact</a>
                            </h6>
                        </div>
                        <!-- Grid column -->
                    </div>
                    <!-- Grid row-->
                </section>
                <!-- Section: Links -->

                <hr class="my-5" />

                <!-- Section: Text -->
                <section class="mb-5">
                    <div class="row d-flex justify-content-center">
                        <div class="col-lg-8 text-dark">
                            <p>
                                Desenvolvido com paixão e raiva em Jundiaí, São Paulo, Brasil.
                            </p>
                        </div>
                    </div>
                </section>
                <!-- Section: Text -->
                <!-- Section: Social -->
                <section class="text-center mb-5">
                    <a href="" class="text-white me-4">
                        <i class="fab fa-facebook-f"></i>
                    </a>
                    <a href="" class="text-white me-4">
                        <i class="fab fa-twitter"></i>
                    </a>
                    <a href="" class="text-white me-4">
                        <i class="fab fa-google"></i>
                    </a>
                    <a href="" class="text-white me-4">
                        <i class="fab fa-instagram"></i>
                    </a>
                    <a href="" class="text-white me-4">
                        <i class="fab fa-linkedin"></i>
                    </a>
                    <a href="" class="text-white me-4">
                        <i class="fab fa-github"></i>
                    </a>
                </section>
                <!-- Section: Social -->
            </div>
            <!-- Grid container -->
            <!-- Copyright -->
            <div class="text-center p-3"
                 style="background-color: rgba(0, 0, 0, 0.2)">
                © Copyright 2025  All Rights Reserved 
                <a class="text-white" href="https://cepheus-web-site.vercel.app/">CePheus Soluções Inovadoras em Tecnologia da Informação</a>
                <br/>
                Desenvolvido por Carlos Eduardo da Silva
            </div>
            <!-- Copyright -->
        </footer>
        <!-- Footer -->
    </div>
    <!-- End of .container -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
